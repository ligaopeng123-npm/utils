{
    "name": "@gaopeng123/utils",
    "version": "0.4.1",
    "private": true,
    "workspaces": [
        "packages/*"
    ],
    "publishConfig": {
        "access": "public"
    },
    "changelog": {
        "labels": {
            "feat": ":rocket: add css-obj and hump functions(4)",
            "bug": ":bug: Bug Fix",
            "doc": ":memo: Documentation",
            "internal": ":house: Internal",
            "breaking": ":boom: Breaking Change"
        }
    },
    "license": "MIT",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "scripts": {
        "dev": "webpack serve",
        "build:test": "webpack build --config webpack.dev.config.js",
        "start": "tsdx watch",
        "build": "lerna exec --ignore=@gaopeng123/utils -- npm run build",
        "test": "lerna exec --ignore=@gaopeng123/utils -- npm run test",
        "push": "lerna publish",
        "mark": "doctoc .",
        "lint": "tsdx lint",
        "prepare": "tsdx build",
        "size": "size-limit",
        "analyze": "size-limit --why",
        "build:md": "webpack build --config webpack.md.config.js",
        "start:md": "docsify serve document",
        "build:docs": "node docs.js"
    },
    "files": [],
    "engines": {
        "node": ">=10"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ligaopeng123-npm/utils.git"
    },
    "husky": {
        "hooks": {
            "pre-commit": "tsdx lint"
        }
    },
    "prettier": {
        "printWidth": 80,
        "semi": true,
        "singleQuote": true,
        "trailingComma": "es5"
    },
    "author": "ligaopeng123",
    "module": "dist/utils.esm.js",
    "size-limit": [
        {
            "path": "dist/utils.cjs.production.min.js",
            "limit": "10 KB"
        },
        {
            "path": "dist/utils.esm.js",
            "limit": "10 KB"
        }
    ],
    "devDependencies": {
        "@size-limit/preset-small-lib": "^5.0.1",
        "@types/node": "^16.4.6",
        "copy-webpack-plugin": "6.4.1",
        "html-webpack-plugin": "^5.5.0",
        "husky": "^7.0.0",
        "lerna": "^4.0.0",
        "size-limit": "^5.0.1",
        "ts-loader": "^9.2.7",
        "tsdx": "^0.14.1",
        "tslib": "^2.3.0",
        "typescript": "^4.3.5",
        "webpack": "^5.70.0",
        "webpack-cli": "^4.9.1",
        "webpack-dev-server": "^4.7.4"
    }
}
